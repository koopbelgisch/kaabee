{
   "type": "sqlite",
   "database": "db/development.sqlite3",
   "synchronize": true,
   "logging": true,
   "entities": [
      "src/models/**/*.ts"
   ],
   "migrations": [
      "db/migrations/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "src/models",
      "migrationsDir": "src/migration"
   }
}
